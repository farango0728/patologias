<!doctype html>
<html lang="es">
<head>
    <link rel="shortcut icon" href="{{ base_path }}/images/favicon.ico">
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" type="text/css" href="{{ base_path }}/css/template.css">
    <meta name="description" content="Imexhs - Bogota Colombia">
    <meta name="author" content="Imexhs - Bogota Colombia">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="{{ base_path }}/fonts/SimpleIcons/css/simple-line-icons.css">
    <link rel="stylesheet" type="text/css" href="{{ base_path }}/css/responsive.css">
    <link rel="stylesheet" type="text/css" href="{{ base_path }}/css/jbility.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.24/css/jquery.dataTables.min.css">
    
    <title>Home Enterprise - Imexh</title>

    {% block style %}{% endblock %}
</head>
<body>
<header>
    <div class="rd-container">
        <div class="rd-element rd-s-100 rd-l-50 t-left">
            <a tabindex="1" href="{{ url_for('home') }}">
                <img src="{{ base_path }}/images/plataforma_digital.png" alt="Logo de imxh" class="logo">
            </a>
        </div>
        {% include "template/nav.twig" %}
        <ul class="oculto"><li><a href="#contenido">Ir al contenido princpal</a></li></ul>
        <div class="rd-element rd-s-100 rd-l-50 t-right rd-element-align-center flex rd-element-justify-flex-end">
            <div tabindex="1"  class="menu" onclick="hideShow()">
                <p>{{ auth.user.nombres }} {{ auth.user.apellidos }}</p>
                <img class="avatar" src="{{ base_path }}/images/f1.jpg" alt="" width="200">
                <i class="icon-arrow-down"/></i>
            </div>
        </div>
    </div>
    <div class="menu-box" onclick="hideShow()">
        <ul>
            <li><a tabindex="1" href="{{ url_for('home') }}"><i class="icon-home"></i> Inicio</a></li>
            <li><a tabindex="1" href="#"><i class="icon-user"></i> Perfil</a></li>
            <li><a tabindex="1" href="#"><i class="icon-envelope"></i> Mensajes</a></li>
            <li>
                <a tabindex="1"  target="_blank" href="#">
                    <i class="icon-support"></i> Centro de soporte
                </a>
            </li>
            <li><a tabindex="1"  href="{{ url_for('logout') }}"><i class="icon-logout"></i> Cerrar sesion</a></li>
        </ul>
    </div>
</header>
{% block content %}{% endblock %}
<footer>
    <div class="rd-container">
        <div class="rd-element rd-s-100 rd-l-40">
            <h4>Comunícate con nosotros</h4>            
            <p>Headquarters Latam</p>
            <p>Edificio Chicó 9211, Ofc. 202 </p>
            <p>Bogotá – Colombia</p>  
            <p>+57(1) 316 48 90</p>
        </div>
        <div class="rd-element rd-s-100 rd-l-60 logos">
            <img src="{{ base_path }}/images/logo_footer.png" alt="Logo de imexh" class="logo  rd-s-100 rd-m-40">
            
        </div>
        
        <div class="rd-element rd-s-100 t-right">
            <span style="font-size: small">© 2021 IMEXHS all rigths reserved  –  PRIVACY POLICY</span>
        </div>
    </div>
</footer>

{% include 'accessibility/accessibility.twig' %}
<script src="//code.jquery.com/jquery-3.1.1.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
<script type="text/javascript" src="https://cdn.datatables.net/v/bs4/dt-1.10.16/datatables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.2/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.print.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.flash.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.2/js/buttons.html5.min.js"></script>
<script src="{{ base_path }}/js/jbility.min.js"></script>
<script src="{{ base_path }}/js/jquery-ui.js"></script>
<script src="{{ base_path }}/js/template.js"></script>
<script src="{{ base_url }}/js/modules/orden.js"></script>
<script src="{{ base_url }}/js/modules/user.js"></script>
<script src="{{ base_url }}/js/modules/eps.js"></script>
<script src="{{ base_url }}/js/modules/modality.js"></script>
 <script>

        $.ajaxSetup({
            // Disable caching of AJAX responses
            cache: true
        });
        $(document).ajaxStart(function () {

            $(document.body).css({
               'cursor' : 'progress'
           });
            $("#block_pantalla").fadeIn();

        }).ajaxStop(function () {
            $(document.body).css({
               'cursor' : 'default'
            });
            $("#block_pantalla").fadeOut();
        });
        var getUri = "{{ base_url }}";
    </script>

</body>
</html>